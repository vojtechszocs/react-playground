{
  "parserOptions": {
    "ecmaVersion": 2015
  },
  "extends": [
    "eslint-config-standard",
    "eslint-config-standard-react"
  ],
  "env": {
    "browser": true
  },
  "globals": {
    "__DEV__": false
  },
  "rules": {
    "generator-star-spacing": ["error", "after"],
    "padded-blocks": ["error", { "switches": "always" }],
    "yield-star-spacing": ["error", "after"]
  }
}
