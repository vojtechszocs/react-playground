{
  "name": "react-playground",
  "version": "1.0.0-rc1",
  "repository": "https://github.com/vojtechszocs/react-playground.git",
  "author": "Vojtech Szocs <vojtech.szocs@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "lint:js": "eslint src build --ext .js,.jsx",
    "lint:css": "stylelint \"src/**/*.css\"",
    "lint": "yarn run lint:js && yarn run lint:css",
    "pretest": "yarn run lint",
    "test": "jest src --config=build/jest.config.js --ci",
    "test:snap": "yarn run test -- --updateSnapshot",
    "test:watch": "yarn run test -- --watch",
    "prebuild": "yarn run test",
    "build": "NODE_ENV=production webpack --config build/webpack.config.js --bail",
    "build:dev": "webpack --config build/webpack.config.js --progress",
    "start:dev": "webpack-dev-server --config build/webpack.config.js --progress --watch",
    "start:api": "json-server --watch tools/test-data.json",
    "precommit": "yarn run test"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chunk-manifest-webpack-plugin": "^1.1.0",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^20.0.4",
    "json-server": "^0.11.2",
    "react": "^15.6.1",
    "react-autobind": "^1.0.6",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-test-renderer": "^15.6.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.15.4",
    "reselect": "^3.0.1",
    "style-loader": "^0.18.2",
    "stylelint": "^7.13.0",
    "stylelint-config-standard": "^16.0.0",
    "tcomb": "^3.2.21",
    "url-loader": "^0.5.9",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0",
    "whatwg-fetch": "^2.0.3"
  },
  "engines": {
    "node": ">=6"
  }
}
